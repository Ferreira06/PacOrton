[sub_resource type="Shader" id="Shader_w4h2f"]
code = "shader_type canvas_item;

// Parâmetro de animação: 1.0 = Invisível (tudo descartado) | 0.0 = Visível (nada descartado)
uniform float dissolve_amount : hint_range(0.0, 1.0) = 1.0; 

// Textura de Ruído: Essencial para o efeito. Atribua um NoiseTexture nela!
uniform sampler2D noise_texture;

void fragment() {
    // 1. Amostra a cor base do texto (que já inclui a cor e alpha do Label)
    vec4 base_color = texture(TEXTURE, UV) * COLOR;
    
    // Se o pixel for transparente, descarte-o imediatamente (otimização)
    if (base_color.a < 0.01) {
        discard;
    }
    
    // 2. Coordenadas de Alta Frequência (Efeito Pixelado/Dither)
    // Multiplicar por um valor alto (e.g., 64.0) faz com que a amostra de ruído 
    // seja muito repetitiva, dando a aparência de blocos ou pixels individuais.
    vec2 high_freq_uv = UV * 64.0;
    
    // 3. Amostra o Ruído (apenas o canal Vermelho)
    // Não adicionamos TIME aqui para que o padrão de aparição seja estático e limpo.
    float noise_value = texture(noise_texture, high_freq_uv).r;

    // 4. Lógica de Dissolve (Threshold)
    // Se o valor de ruído for maior que o dissolve_amount, o pixel aparece.
    // Quando dissolve_amount está em 1.0, quase nenhum ruído é maior, então some.
    // Quando dissolve_amount está em 0.0, a maioria do ruído é maior, então aparece.
    
    if (noise_value > dissolve_amount) {
        COLOR = base_color;
    } else {
        discard; // O pixel é descartado, criando o efeito de esmaecimento pixelado.
    }
}"

